{
  "name": "shoppinglist",
  "version": "1.0.0",
  "description": "Desktop Shopping List App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ShoppingListApp\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "author": "Shivendra TechSter",
  "license": "MIT",
  "dependencies": {
    "electron": "^2.0.3"
  },
  "devDependencies": {
    "electron-packager": "^12.1.0"
  }
}
